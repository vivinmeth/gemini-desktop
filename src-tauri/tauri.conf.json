{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "gemini",
  "version": "0.1.0",
  "identifier": "com.surkyl.gemini-desktop",
  "build": {
    "devUrl": "https://gemini.google.com",
    "frontendDist": "https://gemini.google.com"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Gemini (Desktop)",
        "width": 1366,
        "height": 768
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
